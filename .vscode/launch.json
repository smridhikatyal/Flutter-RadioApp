    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "ai_radioapp",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "ai_radioapp (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "ai_radioapp (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },

        {
                
                "name": "Flutter (sdk gphone64 x86 64)",
                "request": "launch",
                "type": "dart",
                "program": "lib/main.dart",
                "cwd": "${workspaceFolder}",
                "flutterMode": "debug",
                
        },


    ]
}